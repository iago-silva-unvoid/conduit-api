    "fastify": "^3.25.3",
    "fp-ts": "2.9.5",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "jose": "^4.3.7",
    "module-alias": "2.2.2",
    "monocle-ts": "^2.3.11",
    "newtype-ts": "^0.3.4",
    "slugify": "^1.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "26.0.22",
    "@types/module-alias": "2.0.0",
